# Cloudflare R2 配置模板
R2_ACCOUNT_ID=your_account_id_here
R2_ENDPOINT=https://your_account_id.r2.cloudflarestorage.com
R2_BUCKET=your_bucket_name

# Admin权限 (用于批量上传和管理)
R2_ADMIN_ACCESS_KEY=your_admin_access_key
R2_ADMIN_SECRET_KEY=your_admin_secret_key

# User权限 (用于日常文件访问)
R2_USER_ACCESS_KEY=your_user_access_key
R2_USER_SECRET_KEY=your_user_secret_key

# CDN相关
R2_PUBLIC_URL=https://your_account_id.r2.cloudflarestorage.com/your_bucket_name
